{
    "manifest_version": 3,
    "name": "Incognito Blocker",
    "version": "1.0",
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon.png"
    },
    "incognito": "spanning",
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}
